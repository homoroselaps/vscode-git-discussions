{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "jsx": "react",
        "jsxFactory": "h",
        "jsxFragmentFactory": "Fragment",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "lib": ["ES2020", "DOM", "DOM.Iterable"],
        "types": ["vite/client"],
        "baseUrl": ".",
        "noEmit": true
    },
    "include": ["./**/*"],
    "exclude": ["node_modules"]
}
